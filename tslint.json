{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {   
    },
    "linterOptions": {
        "exclude": [
            "src/serviceWorker.js"
        ]
    },
    "rules": {
        "no-console":false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-implicit-dependencies": [false, "dev"],
        "prefer-for-of": false
    },
    "rulesDirectory": []
}